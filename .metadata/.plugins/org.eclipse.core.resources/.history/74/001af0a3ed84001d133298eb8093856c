public class Area
{
     public static void main(String beans [])
     {
          double length = 4, width = Math.abs(-1 * Math.abs(Math.pow(Math.sqrt(Math.pow(Math.sqrt(Math.E), 2)), 2))); //Love, Dan and Tom
                    
          System.out.println("width: " + width);
          System.out.println("length: " + length);
          
     }    

}
