
public class DoubleCompare
{

     public static void main(String args [])
     {
          double root = Math.sqrt(2);
          double result = Math.pow(root, 2) - 2;
          
          if(result == 0)
          {
               System.out.println(result + " = 0");
          }
          else
          {
               System.out.println(result + " != 0");
          }
     }
     
}
